#include <Library.tmpl>

#define MyNormalLibraryTarget(libname,dirs,objs)                      @@\
AllTarget(Concat(lib,libname.a))                                      @@\
                                                                      @@\
Concat(lib,libname.a):                                                @@\
	for i in dirs ; do \                                          @@\
	    echo $$i ; cd $$i ; $(MAKE) ; cd .. ; \                   @@\
	done                                                          @@\
	RemoveFile($@)                                                @@\
	MakeLibrary($@,objs)                                          @@\
	RanLibrary($@)                                                @@\
	_LinkBuildLibrary($@)

#define IHaveSubdirs
#define PassCDebugFlags 'CDEBUGFLAGS=$(CDEBUGFLAGS)'

